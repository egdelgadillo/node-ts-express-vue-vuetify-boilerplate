{
  "root": true,
  "env": {
    "node": true,
    "es2022": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": [
      "error",
      {
        "allow": [
          "info",
          "warn",
          "error",
          "debug",
          "time",
          "timeEnd",
          "timeLog"
        ]
      }
    ],
    "no-unused-vars": "error"
  }
}
